        "f78a8e95-691d-4e23-9112-20d5365734d9": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/7_Grafik-VA_779a60e6-edb8-4da8-8029-856e6de1923f.png",
            "column_size": "3",
            "title": "",
