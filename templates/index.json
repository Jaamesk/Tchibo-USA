        "product-for-buy-button": "the-tchibo-bundle",
        "liquid": "<div class=\"text-with-image-alt\">\n<div class=\"img-column-alt\">\n<img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0618\/9386\/9774\/files\/TheTchiboBundle.png?v=1679327035\">\n<\/div>\n<div class=\"text-column-alt\">\n<p class=\"tp-overline-secondary\">Single Serve espresso Machine<\/p>\n<h1 class=\"tp-display-4\">The Tchibo Brewer's Bundle<\/h1>\n<p class=\"tp-display-1\" style=\"margin:10px 0\">Bean-to-brew Coffee & Espresso Machine + 2 Whole Bean Coffees (17.6 oz. each)<\/p>\n{% render 'price-list', product: section.settings.product-for-buy-button %}\n<p class=\"tp-body\" style=\"padding-top:15px\">Meet the machine that grinds AND brews your coffee in one seamless step. What could be better than that?\n<\/p>\n<p class=\"tp-body\" style=\"padding:15px 0\">This bundle includes our revolutionary bean-to-brew coffee machine as well as a 17.6oz pack of Barista Espresso & Barista Milano Style each to get you started.\n<\/p>\n{% render 'buy-buttons', product: section.settings.product-for-buy-button, show_payment_button: true %}\n<p style=\"padding-top:15px\" class=\"tp-body\"><a href=\"\/products\/the-tchibo-bundle\"><u>See details<\/u><\/a><\/p>\n<\/div>\n<\/div>\n\n<style>\n.text-with-image-alt {\ndisplay:flex;\nalign-items: center;\njustify-content: space-between\n}\n.img-column-alt {\nwidth: 48%;\n}\n.text-column-alt {\nwidth: 48%;\n}\n\n@media only screen and (max-width: 767px) {\n.text-with-image-alt {\nflex-direction:column;\n}\n.img-column-alt {\nwidth:100%;\n}\n.img-column-alt {\npadding-left: 0;\npadding-bottom: 20px;\n}\n.text-column-alt {\nwidth: 100%\n}\n}\n<\/style>",
        "background": "rgba(0,0,0,0)",
        "background_gradient": "",
        "text_color": ""
      }
    },
