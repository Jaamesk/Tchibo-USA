          "settings": {
            "text": "OUR HISTORY",
            "font-style": "tp-overline-secondary"
          }
        },
        "template--15632684318926__bacfc330-f03e-425d-8024-044eee6649a1-16717185938a7459e1-1": {
          "type": "heading",
          "settings": {
            "text": "The History of Tchibo",
            "heading_tag": "tp-display-4",
            "text_color": "",
            "gradient": ""
          }
        },
        "template--15632684318926__bacfc330-f03e-425d-8024-044eee6649a1-16717185938a7459e1-2": {
          "type": "richtext",
          "settings": {
            "content": "<p>Connoisseurs love to understand the origins of their beloved brands.  Here is the story of Europe's #1 coffee house.<\/p>",
            "font-style": "tp-body"
          }
        },
        "b4141356-353f-4523-90ed-3092516813a8": {
          "type": "liquid",
          "settings": {
            "liquid": "<video width=\"auto\" controls>\n<source src=\"https:\/\/cdn.shopify.com\/videos\/c\/o\/v\/b7c86d0ff7e3490588d37e8d7b93c7c9.mp4\" type=\"video\/mp4\">\n<\/video>"
          }
        }
      },
      "block_order": [
        "template--15632684318926__bacfc330-f03e-425d-8024-044eee6649a1-16717185938a7459e1-0",
